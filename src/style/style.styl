#Story iframe
    margin-top 15px
    margin-left 15px

.stamp
    width 160px
    height 80px
    display inline-block
    padding 10px
    background white
    margin-top 80px
    margin-left auto
    margin-right auto
    // position relative
    // top 150px
    // left 100px
    filter drop-shadow(0 0 10px rgba(0,0,0,0.5))
    background radial-gradient( transparent, transparent 6px, #fff 6px, #fff )-10px -10px;
    background-size 20px 20px 
    background-position -10px -10px 
    font-size 1rem
    font-weight bolder

.stamp::after
    content ''
    position absolute
    left 5px
    top 5px
    right 5px
    bottom 5px
    z-index -3

.stamp::before
    content ''
    position absolute
    left 6px
    top 6px
    right 5px
    bottom 5px
    background white
    width 145px
    height 60px
    z-index -2

.text
    color white
    width 100%
    padding 20px
    // position absolute
    // top 430px
    // left 450px
    font-size 12px
    letter-spacing: 3px

