// -----------------------------------xs-----------------------------------------
// ------------------navbar--------------------
.navbar 
    background #D0D0D0
.logo img 
    width 70px

.logo h2
    color white !important
    font-size 20px !important
    line-height 40px
.menu-box
    display none !important
#Home,#Story,#news,#Contact,#where,.new,#back,#member,#dm
