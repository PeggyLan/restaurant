.flipbook::after
        content 'Flip ME !'
        width 230px
        color red
        position absolute
        top -20px
        left 115px
        font-size 30px
        transform rotate(45deg)
        animation shake 1s infinite

@keyframes shake 
    0%
        transform:translateY(10px) rotate(3deg)
    
    50%
        transform:translateY(-10px) rotate(-3deg)
    
    
    100%
        transform:translateY(10px) rotate(3deg)

